    <dc:title>Harry Potter et l'Enfant Maudit</dc:title>
    <dc:creator opf:role="aut" opf:file-as="Rowling, J.K. &amp; Tiffany, John &amp; Thorne, Jack">J.K. Rowling</dc:creator>
